Users/kenyonluce/IdeaProjects/react-exercises/src/components/Header/Header.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/API.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/hooks/useHomeFetch.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/HeroImage/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/HeroImage/HeroImage.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Grid/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Grid/Grid.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Thumb/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Thumb/Thumb.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Spinner/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Spinner/Spinner.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/SearchBar/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/SearchBar/SearchBar.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Button/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Button/Button.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Movie.js",["131","132","133","134","135"],"import React from 'react';\nimport {useParams} from 'react-router-dom';\nimport {API_KEY, IMAGE_BASE_URL, POSTER_SIZE} from \"../config\";\nimport Grid from './Grid';\nimport Spinner from './Spinner';\nimport NoImage from '../images/no_image.jpg';\nimport {useMovieFetch} from \"../hooks/useMovieFetch\";\nimport BreadCrumb from \"./BreadCrumb\";\nimport MovieInfo from \"./MovieInfo\";\nimport MovieInfoBar from \"./MovieInfoBar\";\n\nconst Movie = () => { //using brackets because we want to include logic for fetching info from the API\n    const {movieId} = useParams(); //gives us the id of the movie so we can get info for that soecific movie\n\n    const {state: movie, loading, error} = useMovieFetch(movieId);\n    //you can change the name of a property using ':'\n    if (loading) return <Spinner/>;\n    if (error) return <div>Something went wrong...</div>;\n\n    console.log(movie)\n    return (\n        <>\n            {/*<div>Movie</div>*/}\n            <BreadCrumb movieTitle={movie.original_title}/>\n        {/*    ^^just a small note, the original_title property was not being called on because in my fetch hook I called on the plural version of the function, which was retrieving a list of movies rather than the info for a singular one, this issue was fixed after an hour by removing the 's' :D */}\n            <MovieInfo movie={movie}/>\n            <MovieInfoBar time={movie.runtime} budget={movie.budget} revenue={movie.revenue}/>\n        </>\n    )\n}\n\nexport default Movie;","/Users/kenyonluce/IdeaProjects/react-exercises/src/components/NotFound.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/hooks/useMovieFetch.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/BreadCrumb/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/BreadCrumb/BreadCrumb.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/MovieInfo/index.js",["136"],"import React from 'react';\n\nimport Thumb from '../Thumb';\nimport {IMAGE_BASE_URL, POSTER_SIZE} from \"../../config\";\n\nimport DefaultImage from '../../images/no_image.jpg';\n\nimport {Wrapper, Content, Text} from './MovieInfo.styles';\nimport {useMovieFetch} from \"../../hooks/useMovieFetch\";\n\nconst MovieInfo = ({movie}) => (\n    <Wrapper backdrop={movie.backdrop_path}>\n        <Content>\n            <Thumb\n            image={\n                movie.poster_path\n                ? `${IMAGE_BASE_URL}${POSTER_SIZE}${movie.poster_path}`\n                    : DefaultImage\n            }\n            clickable={false}\n            alt='movie-thumb'\n            />\n            <Text>\n                <h1>{movie.title}</h1>\n                <h3>PLOT</h3>\n                <p>{movie.overview}</p>\n\n                <div className='rating-directors'>\n                    <div>\n                        <h3>RATING</h3>\n                        <div className='score'>{movie.vote_average}</div>\n                    </div>\n                    <div className='director'>\n                        <h3>DIRECTOR{movie.directors.length > 1 ? 'S' : ''}</h3>\n                    {/*    the above expression adds an 's' to the word if if there is more than one director involved with the movie*/}\n                        {movie.directors.map(director => (\n                            <p key={director.credit_id}>{director.name}</p>\n                        ))}\n                    </div>\n                </div>\n            </Text>\n        </Content>\n    </Wrapper>\n);\n\nexport default MovieInfo;","/Users/kenyonluce/IdeaProjects/react-exercises/src/components/MovieInfo/MovieInfo.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/MovieInfoBar/MovieInfoBar.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/helpers.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/MovieInfoBar/index.js",[],{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"141","severity":1,"message":"142","line":3,"column":9,"nodeType":"143","messageId":"144","endLine":3,"endColumn":16},{"ruleId":"141","severity":1,"message":"145","line":3,"column":18,"nodeType":"143","messageId":"144","endLine":3,"endColumn":32},{"ruleId":"141","severity":1,"message":"146","line":3,"column":34,"nodeType":"143","messageId":"144","endLine":3,"endColumn":45},{"ruleId":"141","severity":1,"message":"147","line":4,"column":8,"nodeType":"143","messageId":"144","endLine":4,"endColumn":12},{"ruleId":"141","severity":1,"message":"148","line":6,"column":8,"nodeType":"143","messageId":"144","endLine":6,"endColumn":15},{"ruleId":"141","severity":1,"message":"149","line":9,"column":9,"nodeType":"143","messageId":"144","endLine":9,"endColumn":22},"no-native-reassign",["150"],"no-negated-in-lhs",["151"],"no-unused-vars","'API_KEY' is defined but never used.","Identifier","unusedVar","'IMAGE_BASE_URL' is defined but never used.","'POSTER_SIZE' is defined but never used.","'Grid' is defined but never used.","'NoImage' is defined but never used.","'useMovieFetch' is defined but never used.","no-global-assign","no-unsafe-negation"]
