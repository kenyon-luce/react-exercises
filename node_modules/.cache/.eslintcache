eInfo.styles';\nimport {useMovieFetch} from \"../../hooks/useMovieFetch\";\n\nconst MovieInfo = ({movie}) => (\n    <Wrapper backdrop={movie.backdrop_path}>\n        <Content>\n            <Thumb\n            image={\n                movie.poster_path\n                ? `${IMAGE_BASE_URL}${POSTER_SIZE}${movie.poster_path}`\n                    : DefaultImage\n            }\n            clickable={false}\n            alt='movie-thumb'\n            />\n            <Text>\n                <h1>{movie.title}</h1>\n                <h3>PLOT</h3>\n                <p>{movie.overview}</p>\n\n                <div className='rating-directors'>\n                    <div>\n                        <h3>RATING</h3>\n                        <div className='score'>{movie.vote_average}</div>\n                    </div>\n                    <div className='director'>\n                        <h3>DIRECTOR{movie.directors.length > 1 ? 'S' : ''}</h3>\n                    {/*    the above expression adds an 's' to the word if if there is more than one director involved with the movie*/}\n                        {movie.directors.map(director => (\n                            <p key={director.credit_id}>{director.name}</p>\n                        ))}\n                    </div>\n                </div>\n            </Text>\n        </Content>\n    </Wrapper>\n);\n\nexport default MovieInfo;","/Users/kenyonluce/IdeaProjects/react-exercises/src/components/MovieInfoBar/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/BreadCrumb/index.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/API.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Spinner/Spinner.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/HeroImage/HeroImage.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/Grid/Grid.styles.js",[],"/Users/kenyonluce/IdeaProjects/react-exercises/src/components/SearchBar/SearchBar.styles.js",[]